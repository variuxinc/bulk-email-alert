<?xml version="1.0" ?>
<form xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Ui:etc/ui_configuration.xsd">
	<argument name="data" xsi:type="array">
		<item name="js_config" xsi:type="array">
			<item name="provider" xsi:type="string">variux_emailnotification_bulkemaillogs_form.bulkemaillogs_form_data_source</item>
		</item>
		<item name="label" xsi:type="string" translate="true">General Information</item>
		<item name="template" xsi:type="string">templates/form/collapsible</item>
	</argument>
	<settings>
		<buttons>
			<button name="back" class="Variux\EmailNotification\Block\Adminhtml\BulkEmailLogs\Edit\BackButton"/>
			<button name="delete" class="Variux\EmailNotification\Block\Adminhtml\BulkEmailLogs\Edit\DeleteButton"/>
			<button name="save" class="Variux\EmailNotification\Block\Adminhtml\BulkEmailLogs\Edit\SaveButton"/>
			<button name="save_and_continue" class="Variux\EmailNotification\Block\Adminhtml\BulkEmailLogs\Edit\SaveAndContinueButton"/>
		</buttons>
		<namespace>variux_emailnotification_bulkemaillogs_form</namespace>
		<dataScope>data</dataScope>
		<deps>
			<dep>variux_emailnotification_bulkemaillogs_form.bulkemaillogs_form_data_source</dep>
		</deps>
	</settings>
	<dataSource name="bulkemaillogs_form_data_source">
		<argument name="data" xsi:type="array">
			<item name="js_config" xsi:type="array">
				<item name="component" xsi:type="string">Magento_Ui/js/form/provider</item>
			</item>
		</argument>
		<settings>
			<submitUrl path="*/*/save"/>
		</settings>
		<dataProvider name="bulkemaillogs_form_data_source" class="Variux\EmailNotification\Model\BulkEmailLogs\DataProvider">
			<settings>
				<requestFieldName>bulkemaillogs_id</requestFieldName>
				<primaryFieldName>bulkemaillogs_id</primaryFieldName>
			</settings>
		</dataProvider>
	</dataSource>
	<fieldset name="general">
		<settings>
			<label>General</label>
		</settings>
		<field name="subject" formElement="input" sortOrder="10">
			<argument name="data" xsi:type="array">
				<item name="config" xsi:type="array">
					<item name="source" xsi:type="string">BulkEmailLogs</item>
				</item>
			</argument>
			<settings>
				<dataType>text</dataType>
				<label translate="true">subject</label>
				<dataScope>subject</dataScope>
				<validation>
					<rule name="required-entry" xsi:type="boolean">false</rule>
				</validation>
			</settings>
		</field>
		<field name="content" formElement="textarea" sortOrder="20">
			<argument name="data" xsi:type="array">
				<item name="config" xsi:type="array">
					<item name="source" xsi:type="string">BulkEmailLogs</item>
				</item>
			</argument>
			<settings>
				<dataType>text</dataType>
				<label translate="true">content</label>
				<dataScope>content</dataScope>
				<validation>
					<rule name="required-entry" xsi:type="boolean">false</rule>
				</validation>
			</settings>
		</field>
		<field name="sender" formElement="input" sortOrder="30">
			<argument name="data" xsi:type="array">
				<item name="config" xsi:type="array">
					<item name="source" xsi:type="string">BulkEmailLogs</item>
				</item>
			</argument>
			<settings>
				<dataType>text</dataType>
				<label translate="true">sender</label>
				<dataScope>sender</dataScope>
				<validation>
					<rule name="required-entry" xsi:type="boolean">false</rule>
				</validation>
			</settings>
		</field>
		<field name="recipient" formElement="input" sortOrder="40">
			<argument name="data" xsi:type="array">
				<item name="config" xsi:type="array">
					<item name="source" xsi:type="string">BulkEmailLogs</item>
				</item>
			</argument>
			<settings>
				<dataType>text</dataType>
				<label translate="true">recipient</label>
				<dataScope>recipient</dataScope>
				<validation>
					<rule name="required-entry" xsi:type="boolean">false</rule>
				</validation>
			</settings>
